    .cloud{
      opacity: 0.8;
      position: fixed;
      width: 100%;
      display: none;
    }
    @keyframes cloud-1 {
      from {margin-left: -151px;}
      to {margin-left: 100%;}
    }
    #cloud-1{
        top: 70px;
        -webkit-animation: cloud-1 76s linear infinite;
        animation: cloud-1 76s linear infinite;
    }
    @keyframes cloud-2 {
      from {margin-left: -1502px;}
      to {margin-left: 100%;}
    }
    #cloud-2{
        /*top: 140px;*/
